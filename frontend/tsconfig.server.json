{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "target": "es2017",
    "isolatedModules": false,
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    ".next",
    "next.config.js",
    "next.raw-config.js",
    "dist"
  ],
  "include": [
    "server/**/*.ts"
  ]
}
