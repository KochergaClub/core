{
  "presets": [
    "@babel/env",
    "@babel/react",
    [
      "@babel/typescript",
      { "isTSX": true, "allExtensions": true }
    ]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-proposal-class-properties", {"loose": true}],
    "@babel/plugin-transform-runtime"
  ]
}
